{
  "name": "vm-manager-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "up": "yarn upgrade --latest",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint --max-warnings=1 ./src/**/*.{ts,tsx} --fix && yarn format:fix",
    "lint:strict": "eslint --max-warnings=0 ./src/**/*.{ts,tsx}",
    "type-check": "tsc --noEmit --incremental false",
    "test:ci": "cypress run --config video=false,screenshotOnRunFailure=false",
    "test:open": "cypress open",
    "format:fix": "prettier -w src/**/*.{js,ts,tsx,json,scss,md}",
    "format:check": "prettier -c src/**/*.{js,ts,tsx,json,scss,md}",
    "release": "standard-version",
    "release:major": "standard-version --release-as major",
    "push-release": "git push --follow-tags origin main",
    "postbuild": "next-sitemap",
    "prepare": "husky install",
    "container:build": "docker build -t vm-manager .",
    "container:push": "heroku container:push web --app vm-manager",
    "container:release": "heroku container:release web --app vm-manager",
    "container:deploy": "yarn container:push && yarn container:release",
    "container:run": "docker compose -f docker-compose.yml up"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.4.9",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@next/font": "13.1.6",
    "framer-motion": "^9.0.2",
    "next": "13.1.6",
    "next-connect": "^0.13.0",
    "next-secure-headers": "^2.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.1",
    "react-icons": "^4.4.0",
    "sass": "^1.58.0",
    "sharp": "^0.31.3",
    "swr": "^2.0.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@next/env": "*",
    "@svgr/webpack": "^6.5.1",
    "@types/node": "18.13.0",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "cypress": "^12.3.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^45.0.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "next-sitemap": "^3.1.52",
    "prettier": "^2.8.4",
    "standard-version": "^9.5.0",
    "typescript": "^4.9.5",
    "webpack": "^5.74.0"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "src/**/*.{js,ts,tsx,json,css,scss,md}": [
      "prettier -w"
    ]
  }
}
